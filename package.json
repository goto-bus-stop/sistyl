{
  "name": "sistyl",
  "description": "Simple JSON-to-CSS style sheet library",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "style",
    "css"
  ],
  "repository": "goto-bus-stop/sistyl",
  "author": "goto-bus-stop <rene@kooi.me>",
  "main": "lib/sistyl",
  "dependencies": {
    "split-selector": "^1.0.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
